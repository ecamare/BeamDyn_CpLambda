; DTU 10 MW  
begin simulation;
  time_stop   30.00;
  solvertype   2 ;    (newmark)
  on_no_convergence continue ;
  convergence_limits 1.0E-4 1.0E-3 1E-8 ;
;  logfile ./logfiles/fdll.log ;
  log_deltat 1;
  ;animation sim_anim.dat;
;
  begin newmark;
    deltat    0.01; 
  end newmark;
end simulation;
