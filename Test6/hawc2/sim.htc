begin orientation;
    begin base;
      mbdy   dummy1;
      inipos        0.0 0.0 -0.1;
      body_eulerang 0.0 0.0 0.0;
    end base;
;
    begin relative;
      body1	dummy1 last;
      body2	beam1 1;
      body2_eulerang	0 180 0;
    end relative;    
end orientation;
;-------------------------------------------------------------------------------------------------------------------------------
begin constraint;   
    begin fix0;
      mbdy dummy1;
    end fix0;
;
    begin bearing3;
      name bearing_3;
      mbdy1 dummy1 last;
      mbdy2 beam1 1;
      bearing_vector 2 0.0 1.0 0.0; coordinate system (0 or 1 or 2), rotation vector (x,y,z)
      omegas 0.5; rad/s
    end bearing3;
end constraint;
